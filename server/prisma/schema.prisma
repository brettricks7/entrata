generator client {
	provider = "prisma-client-js"
}

datasource db {
	provider = "postgresql"
	url      = env("DATABASE_URL")
}

model QuizAttempt {
	id           String   @id @default(cuid())
	createdAt    DateTime @default(now())
	topic        String
	score        Int
	total        Int
	answersJson  Json
}
